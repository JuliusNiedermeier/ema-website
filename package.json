{
  "name": "ema-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "mkdir -p generated/sanity && pnpm typegen && pnpm next build",
    "build:local": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\"",
    "typegen": "sanity schema extract --path generated/sanity/schema.json && sanity typegen generate",
    "email": "email dev --dir src/emails",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx ./scripts/migrate-database.ts",
    "db:introspect": "drizzle-kit introspect",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.9.3",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-slot": "^1.0.2",
    "@react-email/components": "0.0.19",
    "@sanity/color-input": "^3.1.1",
    "@sanity/image-url": "^1.0.2",
    "@sanity/vision": "^3.46.1",
    "@t3-oss/env-nextjs": "^0.10.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.31.2",
    "drizzle-zod": "^0.5.1",
    "framer-motion": "^11.2.10",
    "lucide-react": "^0.395.0",
    "next": "14.2.4",
    "next-sanity": "^9.3.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-email": "^2.1.4",
    "resend": "^3.3.0",
    "sanity": "^3.46.1",
    "sharp": "^0.33.4",
    "styled-components": "^6.1.11",
    "tailwind-merge": "^2.3.0",
    "zod": "^3.23.8",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@next/env": "^14.2.4",
    "@types/node": "^20.14.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "drizzle-kit": "^0.22.7",
    "eslint": "^9.5.0",
    "eslint-config-next": "14.2.4",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.2"
  }
}
