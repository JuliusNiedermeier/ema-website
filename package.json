    "drizzle-orm": "^0.31.4",
    "drizzle-zod": "^0.5.1",
    "framer-motion": "^11.5.6",
    "lucide-react": "^0.395.0",
    "next": "14.2.4",
    "next-sanity": "^9.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-email": "^2.1.6",
    "react-turnstile": "^1.1.3",
    "resend": "^3.5.0",
    "sanity": "^3.57.4",
    "sharp": "^0.33.5",
    "styled-components": "^6.1.13",
    "tailwind-merge": "^2.5.2",
    "vaul": "^0.9.4",
